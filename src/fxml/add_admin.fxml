<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.effect.Glow?>
<?import javafx.scene.effect.InnerShadow?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Font?>

<StackPane fx:id="root" prefHeight="720.0" prefWidth="1280.0" style="-fx-background-color: #809baa;" xmlns="http://javafx.com/javafx/8.0.162-ea" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <ImageView fx:id="logoU" fitHeight="130.0" fitWidth="130.0" pickOnBounds="true" preserveRatio="true" translateX="-57.0" translateY="-250.0">
            <image>
                <Image url="@../resources/u.png" />
            </image>
        </ImageView>
        <ImageView fx:id="logoB" fitHeight="130.0" fitWidth="130.0" pickOnBounds="true" preserveRatio="true" translateX="48.0" translateY="-250.0">
            <image>
                <Image url="@../resources/b.png" />
            </image>
        </ImageView>
        <ImageView fx:id="logoI" fitHeight="130.0" fitWidth="130.0" pickOnBounds="true" preserveRatio="true" translateX="124.0" translateY="-250.0">
            <image>
                <Image url="@../resources/i.png" />
            </image>
        </ImageView>
        <ImageView fx:id="logoO" fitHeight="130.0" fitWidth="130.0" pickOnBounds="true" preserveRatio="true" translateX="210.0" translateY="-250.0">
            <image>
                <Image url="@../resources/o.png" />
            </image>
        </ImageView>
        <ImageView fx:id="logoD" fitHeight="130.0" fitWidth="130.0" pickOnBounds="true" preserveRatio="true" translateX="-175.0" translateY="-250.0">
            <image>
                <Image url="@../resources/d.png" />
            </image>
        </ImageView>
        <TextField fx:id="email" alignment="CENTER" maxWidth="300.0" promptText="email" style="-fx-border-color: #1e4768; -fx-border-width: 5px; -fx-background-color: #dbd8c6; -fx-border-radius: 12 12 12 12; -fx-background-radius: 18 18 18 18;" translateY="-80">
            <font>
                <Font name="Helvetica Neue" size="20.0" />
            </font>
        </TextField>
        <PasswordField fx:id="password" alignment="CENTER" maxWidth="300.0" promptText="password" style="-fx-border-color: #1e4768; -fx-border-width: 5px; -fx-background-color: #dbd8c6; -fx-border-radius: 12 12 12 12; -fx-background-radius: 18 18 18 18;" translateY="-20.0">
            <font>
                <Font name="Helvetica Neue" size="20.0" />
            </font>
        </PasswordField>
        <Button fx:id="submitBtn" mnemonicParsing="false" onAction="#handleSubmitBtnClick" prefHeight="100.0" prefWidth="200.0" style="-fx-background-color: #dbd8c6; -fx-background-radius: 25 25 25 25; -fx-border-color: #000000; -fx-border-width: 10px; -fx-border-radius: 20 20 20 20; -fx-border-color: #1e4768;" text="Submit" textFill="#03142b" translateX="-110.0" translateY="250.0">
            <font>
                <Font name="Helvetica Neue" size="25.0" />
            </font>
            <effect>
                <InnerShadow />
            </effect>
            <cursor>
                <Cursor fx:constant="CLOSED_HAND" />
            </cursor>
        </Button>
        <Button fx:id="backBtn" mnemonicParsing="false" onAction="#handleBackBtnClick" prefHeight="100.0" prefWidth="200.0" style="-fx-background-color: #dbd8c6; -fx-background-radius: 25 25 25 25; -fx-border-color: #000000; -fx-border-width: 10px; -fx-border-radius: 20 20 20 20; -fx-border-color: #1e4768;" text="Terug" textFill="#03142b" translateX="110.0" translateY="250.0">
            <font>
                <Font name="Helvetica Neue" size="25.0" />
            </font>
            <effect>
                <InnerShadow />
            </effect>
            <cursor>
                <Cursor fx:constant="CLOSED_HAND" />
            </cursor>
        </Button>
        <Label fx:id="topLabel" text="Nieuwe Beheerder" textFill="#03142b" translateY="-145.0">
            <font>
                <Font name="Helvetica Neue" size="30.0" />
            </font>
            <effect>
                <Glow level="0.5" />
            </effect>
        </Label>
        <Label text="Account Rechten" textFill="#03142b" translateY="50.0">
            <font>
                <Font name="Helvetica Neue" size="30.0" />
            </font>
            <effect>
                <Glow level="0.5" />
            </effect>
        </Label>
        <Slider fx:id="rightsSlider" blockIncrement="1.0" cache="true" majorTickUnit="1.0" max="3.0" maxWidth="300.0" min="1.0" minorTickCount="0" showTickLabels="false" showTickMarks="true" snapToTicks="true" translateY="100" value="1.0" />
        <Label fx:id="rightsText" text="Student" textFill="#03142b" translateY="150.0">
            <font>
                <Font name="Helvetica Neue" size="30.0" />
            </font>
            <effect>
                <Glow level="0.5" />
            </effect>
        </Label>
        <StackPane fx:id="noticePane" maxHeight="200.0" maxWidth="500.0" minHeight="200.0" minWidth="500.0" mouseTransparent="true" opacity="0.0" prefHeight="200.0" prefWidth="500.0" style="-fx-background-color: #dbd8c6; -fx-border-color: #03142b; -fx-border-radius: 30 30 30 30; -fx-background-radius: 35 35 35 35; -fx-border-width: 10px;">
            <children>
                <Label fx:id="noticeLabel" text="PLACEHOLDER" textFill="#03142b" translateY="-20.0">
                    <font>
                        <Font name="Helvetica Neue" size="20.0" />
                    </font>
                </Label>
                <Button fx:id="noticeBtn" maxHeight="0.0" maxWidth="100.0" mnemonicParsing="false" onAction="#hideNotice" style="-fx-border-radius: 30 30 30 30; -fx-border-color: #1e476b; -fx-background-radius: 35 35 35 35; -fx-border-width: 7px; -fx-background-color: #e2b52b;" text="OK" textFill="#03142b" translateY="55.0">
                    <font>
                        <Font name="Helvetica Neue" size="20.0" />
                    </font>
                    <cursor>
                        <Cursor fx:constant="CLOSED_HAND" />
                    </cursor>
                </Button>
            </children>
        </StackPane>
      <Pane fx:id="fillPane" mouseTransparent="true" opacity="0.0" prefHeight="200.0" prefWidth="200.0" style="-fx-background-color: #809baa;" />
    </children>
</StackPane>
